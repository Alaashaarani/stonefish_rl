		<look name="lower_hull" gray="1.0" roughness="0.3" texture="/home/cirs_alaa/repositories/stonefish_rl/Resources/g500/data/girona500/lower_hull_tex.png"/>
		<look name="gray" gray="0.5" roughness="0.4" metalness="0.5"/>
		<look name="tank" rgb="0.9 0.9 0.9" roughness="0.9"/>
